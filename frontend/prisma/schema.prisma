generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model crawler_state {
  id                 Int      @id
  last_run_timestamp DateTime @db.Timestamptz(6)
}

model issues {
  id             BigInt    @id
  node_id        String    @unique
  url            String?
  repository_url String?
  title          String?
  user_login     String?
  state          String?
  project_name   String?
  language       String?
  labels         Json?
  created_at     DateTime? @db.Timestamptz(6)
  updated_at     DateTime? @db.Timestamptz(6)
  body           String?
}